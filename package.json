{
  "name": "owasp-checklist",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm -r --parallel run dev",
    "build": "pnpm -r run build",
    "start": "pnpm -r run start",
    "migrate": "pnpm -C prisma exec prisma migrate dev",
    "seed": "pnpm -C prisma exec prisma db seed",
    "test": "pnpm -r --parallel run test",
    "test:e2e": "pnpm -C apps/web exec playwright test",
    "lint": "pnpm -r --parallel run lint",
    "format": "pnpm -r --parallel run format"
  },
  "devDependencies": {
    "typescript": "^5.5.0"
  }
}
